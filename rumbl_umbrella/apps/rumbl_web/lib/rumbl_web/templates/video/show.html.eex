<h1>Show Video</h1>

<ul>

  <li>
    <strong> url:</strong>
    <%= @video. url %>
  </li>

  <li>
    <strong> title:</strong>
    <%= @video. title %>
  </li>

  <li>
    <strong> description:</strong>
    <%= @video. description %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.video_path(@conn, :edit, @video) %></span>
<span><%= link "Back", to: Routes.video_path(@conn, :index) %></span>
